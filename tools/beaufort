echo "0    0 - <1 "       # 1
echo "1    1 - <4 "       # 3
echo "2    4 - <7 "       # 3
echo "3    7 - <11"       # 4
echo "4   11 - <16"       # 5
echo "5   16 - <22"       # 6
echo "6   22 - <28"       # 6
echo "7   28 - <34"       # 6
echo "8   34 - <41"       # 7
echo "9   41 - <48"       # 7
echo "10  48 - <56"       # 8
echo "11  56 - <64"       # 8
echo "12  >= 64"
